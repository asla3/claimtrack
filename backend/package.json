{
  "name": "claimtrack-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "cross-env TS_NODE_BASEURL=./dist node --require tsconfig-paths/register dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"ts-node-dev -r tsconfig-paths/register --transpile-only src/index.ts\"",
    "format": "npx prettier . --write",
    "lint": "npx eslint .",
    "pre-commit": "npx lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "lint-staged": {
    ".{js,jsx,ts,tsx}": "eslint --fix",
    "!(*)": "prettier --write"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@prisma/extension-accelerate": "^1.2.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.1",
    "prisma": "^6.5.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  }
}
